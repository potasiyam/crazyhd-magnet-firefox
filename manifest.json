{

  "manifest_version": 2,
  "name": "CrazyHD Magnetter",
  "version": "1.0",

  "description": "Adds magnets for CrazyHD.",

  "icons": {
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.crazyhd.com/*"],
      "js": ["core.js"]
    }
  ]

}
